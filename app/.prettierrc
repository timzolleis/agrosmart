{
  "arrowParens": "always",
  "printWidth": 100,
  "jsxSingleQuote": true,
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "bracketSameLine": true,
  "bracketSpacing": true,
  "trailingComma": "all",
  "importOrder": [
    "^react-router",
    "^.\\/+types/\\/(.*)$",
    "^~/components/(.*)$",
    "^~/actions/(.*)$",
    "^~/utils/(.*)$",
    "^~/(.*)$",
    "<THIRD_PARTY_MODULES>"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "plugins": [
    "prettier-plugin-tailwindcss",
    "@trivago/prettier-plugin-sort-imports"
  ]
}